{
    "name": "Priscilla Secure Test",
    "integrity": {
        "modules": [
            {
                "name": "vanguard",
                "configuration": {
                    "features": {
                        "foreground_window": true
                    }
                }
            },
            {
                "name": "supervisor",
                "configuration": {
                    "backend": "http"
                }
            }
        ]
    },
    "webprofiles": [
        {
            "name": "priscilla",
