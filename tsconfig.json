{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "target": "esnext",
        "module": "CommonJS",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "outDir": "./dist",
        "plugins": [
            { "transform": "typia/lib/transform" }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}